<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link rel="stylesheet" href="/public/styles.css" />
  </head>
  <body>
    <main class="card">
      <h1><%= title %></h1>
      <p><%= message %></p>
      <ul>
        <% items.forEach(function(item) { %>
          <li><%= item %></li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
